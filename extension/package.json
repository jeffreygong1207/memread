{
  "name": "memread-extension",
  "description": "Universal Memory Bridge for LLMs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
